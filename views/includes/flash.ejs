<%if(success && success.length){%>
<div class="alert alert-primary alert-dismissible fade show col-6 offset-3" role="alert" ><%=success%></div>
<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
<%}%>

<%if(error && error.length){%>
<div class="alert alert-primary alert-dismissible fade show col-6 offset-3" role="alert" ><%=error%></div>
<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
<%}%>
